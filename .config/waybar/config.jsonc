        "format": "{:%a %d %b %H:%M:%S}",
        "interval": 1,
        "tooltip-format": "<span size='9pt' font='Mono'>{calendar}</span>",
        "calendar": {
            "mode": "month", // month | year
            "mode-mon-col": 3,
            // "weeks-pos": "right",
